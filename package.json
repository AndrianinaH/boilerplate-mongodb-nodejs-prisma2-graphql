{
  "name": "boilerplate-mongodb-nodejs-prisma2-graphql",
  "version": "1.0.0",
  "description": "backend graphql nodejs prisma2 mongodb",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --transpile-only ./src/index.ts",
    "build": "tsc --build tsconfig.json",
    "graphqlgen": "graphqlgen",
    "generate:graphql": "get-graphql-schema http://localhost:4000 > fullschema.graphql && graphqlgen"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndrianinaH/boilerplate-mongodb-nodejs-prisma2-graphql.git"
  },
  "keywords": [
    "prisma2",
    "graphql",
    "mongodb",
    "nodejs"
  ],
  "author": "AndrianinaH",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AndrianinaH/boilerplate-mongodb-nodejs-prisma2-graphql/issues"
  },
  "homepage": "https://github.com/AndrianinaH/boilerplate-mongodb-nodejs-prisma2-graphql#readme",
  "dependencies": {
    "apollo-server": "^2.16.1",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-import": "^1.0.2",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.3.4",
    "graphql-tools": "^6.0.18",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^11.0.0",
    "graphqlgen": "^0.6.0-rc9",
    "merge-graphql-schemas": "^1.7.8",
    "typescript": "^3.9.7",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^14.0.27",
    "@types/uuid": "^8.3.0",
    "get-graphql-schema": "^2.1.2",
    "ts-node-dev": "^1.0.0-pre.57",
    "yarn": "^1.22.4"
  }
